{
  "name": "react-native-base",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios --scheme MIA",
    "ios:develop": "react-native run-ios --scheme MIA-Develop",
    "ios:staging": "react-native run-ios --scheme MIA-Staging",
    "android": "react-native run-android --variant=productionDebug",
    "android:develop": "react-native run-android --variant=developDebug --appIdSuffix=dev",
    "android:staging": "react-native run-android --variant=stagingDebug --appIdSuffix=staging",
    "android:apk:develop": "cd android && ./gradlew assembleDevRelease",
    "android:apk:staging": "cd android && ./gradlew assembleStagingRelease",
    "android:apk": "cd android && ./gradlew assembleProductionRelease",
    "android:bundle:develop": "cd android && ./gradlew bundleDevRelease",
    "android:bundle:staging": "cd android && ./gradlew bundleStagingRelease",
    "android:bundle": "cd android && ./gradlew bundleProductionRelease",
    "test": "jest",
    "test:cover": "yarn run test --coverage",
    "prettier": "prettier --write './src/**/*.js'"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.8.0",
    "@react-native-community/async-storage": "1.7.1",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-community/picker": "^1.6.6",
    "@react-native-community/progress-bar-android": "^1.0.3",
    "@react-native-community/progress-view": "^1.2.2",
    "@react-navigation/bottom-tabs": "^5.6.1",
    "@react-navigation/native": "^5.0.0",
    "@react-navigation/stack": "^5.0.0",
    "@rootstrap/redux-tools": "^0.4.2",
    "add": "^2.0.6",
    "axios": "^0.19.2",
    "eslint-plugin-prettier": "3.1.0",
    "humps": "2.0.1",
    "immer": "5.0.0",
    "lodash": "4.17.15",
    "moment": "^2.27.0",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-blob-util": "^0.16.2",
    "react-native-config": "0.12.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-gesture-handler": "^1.5.6",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-launch-navigator": "^1.0.8",
    "react-native-localization": "2.1.5",
    "react-native-multiple-select": "^0.5.5",
    "react-native-onesignal": "OneSignal/react-native-onesignal#5e7380a4",
    "react-native-pdf": "^6.2.2",
    "react-native-permissions": "^3.0.0",
    "react-native-picker-select": "^8.0.0",
    "react-native-progress-circle": "^2.1.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-rename": "^2.4.1",
    "react-native-safe-area-context": "^3.0.2",
    "react-native-screens": "^2.0.0-beta.2",
    "react-native-svg": "^12.1.0",
    "react-native-vector-icons": "^7.1.0",
    "react-navigation": "^4.4.0",
    "react-redux": "7.1.1",
    "redux": "4.0.4",
    "redux-form": "8.2.3",
    "redux-persist": "^6.0.0",
    "reselect": "4.0.0",
    "validate.js": "0.12.0",
    "yarn": "^1.22.5"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@react-native-community/eslint-config": "0.0.7",
    "@testing-library/react-native": "^5.0.3",
    "axios-mock-adapter": "^1.17.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "3.2.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "husky": "3.0.1",
    "isomorphic-fetch": "2.2.1",
    "jest": "^25.1.0",
    "lint-staged": "9.2.0",
    "metro-react-native-babel-preset": "0.58.0",
    "prettier": "1.18.2",
    "react-dom": "16.9.0",
    "react-test-renderer": "16.9.0",
    "redux-logger": "3.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "yarn lint:fix --quiet",
      "git add"
    ]
  }
}
